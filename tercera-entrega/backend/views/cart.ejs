<div style="display:flex; width:100%; height:100px; gap:20px">
  <a href="/">home</a>
  <a href="/account">account</a>
  <a href="/cart">cart</a>
  <a href="/auth/logout">logout</a>
</div>

<h1>carrito</h1>

<% if(prods.length > 0) { %>
  <a href="/cart/empty" >vaciar carrito</a>
  <div>
    <% prods.map(el => { %> 
      <div style="width: 800px; display:flex; flex-direction: row; gap:20px">
        <img style="height:200px; object-fit: contain;" src=<%= el.image %> />
        <div>
          <h2> <%= el.title %> </h2>
          <p> $<%= el.price %> </p>
          <p> cantidad: <%= el.quantity %> </p>
        </div>
      </div>
    <% }) %>
  </div>
<% } else {%>
  <h1>no hay productos en el carrito</h1>
  <% } %>